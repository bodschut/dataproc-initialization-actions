    "PYTHONPATH": "/usr/lib/spark/python/:/usr/lib/spark/python/lib/py4j-0.10.1-src.zip",
    "PYTHONSTARTUP": "/usr/lib/spark/python/pyspark/shell.py",
    "PYSPARK_SUBMIT_ARGS": "--master yarn --deploy-mode client --packages com.databricks:spark-csv_2.10:1.4.0,com.databricks:spark-avro_2.11:3.0.0 pyspark-shell"
 }
}

